<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="jason.ss.tao.entity">
	<class name="Composite">
		<composite-id name="compositePK">
			<key-property name="compositeId1" column="COMPOSITE_ID_1" />
			<key-property name="compositeId2" column="COMPOSITE_ID_2" />
		</composite-id>
		<property name="name" length="20" not-null="true" />
	</class>
</hibernate-mapping>
